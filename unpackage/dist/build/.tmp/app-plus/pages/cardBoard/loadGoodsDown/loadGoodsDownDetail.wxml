<view><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">保存出仓</view></cu-custom><form><view data-ref="elForm" class="elForm vue-ref"><view class="cu-form-group border-top"><view class="title">车牌:</view><input disabled placeholder="请选择车牌" name="input" data-event-opts="{{[['tap',[['setSearchDataListSource',['LicensePlate']]]],['input',[['__set_model',['$0','LicensePlate','$event',[]],['formItem']]]]]}}" value="{{formItem.LicensePlate}}" bindtap="__e" bindinput="__e"/><text data-event-opts="{{[['tap',[['setSearchDataListSource',['LicensePlate']]]]]}}" class="cuIcon-rounddown text-green" bindtap="__e"></text></view><view class="cu-form-group border-top"><view class="title">司机:</view><input disabled placeholder="请选择司机" name="input" data-event-opts="{{[['tap',[['setSearchDataListSource',['driver']]]],['input',[['__set_model',['$0','driver','$event',[]],['formItem']]]]]}}" value="{{formItem.driver}}" bindtap="__e" bindinput="__e"/><text data-event-opts="{{[['tap',[['setSearchDataListSource',['driver']]]]]}}" class="cuIcon-rounddown text-green" bindtap="__e"></text></view><view class="cu-form-group border-top"><view class="title">装车:</view><input disabled placeholder="请选择装车" name="input" data-event-opts="{{[['tap',[['setSearchDataListSource',['entrucking']]]],['input',[['__set_model',['$0','entrucking','$event',[]],['formItem']]]]]}}" value="{{formItem.entrucking}}" bindtap="__e" bindinput="__e"/><text data-event-opts="{{[['tap',[['setSearchDataListSource',['entrucking']]]]]}}" class="cuIcon-rounddown text-green" bindtap="__e"></text></view><view class="cu-form-group border-top"><view class="title">发货:</view><input disabled placeholder="请选择发货" name="input" data-event-opts="{{[['tap',[['setSearchDataListSource',['sendGoods']]]],['input',[['__set_model',['$0','sendGoods','$event',[]],['formItem']]]]]}}" value="{{formItem.sendGoods}}" bindtap="__e" bindinput="__e"/><text data-event-opts="{{[['tap',[['setSearchDataListSource',['sendGoods']]]]]}}" class="cuIcon-rounddown text-green" bindtap="__e"></text></view><view class="cu-form-group margin-top"><view class="title">自提</view><switch class="{{[formItem.isSelfTake?'checked':'']}}" checked="{{formItem.isSelfTake?true:false}}" data-event-opts="{{[['change',[['isSelfTakeChange',['$event']]]]]}}" bindchange="__e"></switch></view><view class="cu-form-group margin-top"><view class="title">送货日期</view><picker mode="date" value="{{formItem.deliverDate}}" start="2015-09-01" end="2020-09-01" data-event-opts="{{[['change',[['DateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+formItem.deliverDate+''}}</view></picker></view></view></form><view style="{{'height:'+(tableHeight-10+'px')+';'}}"></view><view data-ref="elBtn" class="elBtn margin-text-center vue-ref"><view class="text-red"><text>保存出仓自动生成送货单，
						 不能再进行修改，
						 如需修改明细请于ERP送货单中进行修改<view style="margin-top:20rpx;" class="_p"></view></text></view><view class="flex  p-xs "><view class="flex-twice  radius"></view><view class="flex-sub radius"></view><view class="flex-twice  radius"><button class="cu-btn block bg-green  lg" loading="{{false}}" disabled="{{!canSubmit||CarListNo==''||formItem.LicensePlate==''||formItem.entrucking==''||formItem.driver==''||formItem.sendGoods==''}}" type data-event-opts="{{[['tap',[['saveWarehouses']]]]}}" bindtap="__e">保存出仓</button></view></view></view><alert-box class="vue-ref" vue-id="2" msgContent="{{msgContent}}" toShowModal="{{toShowModal}}" data-ref="alertBox" data-event-opts="{{[['^cancelModal',[['cancelModal']]],['^comfirmModal',[['comfirmModal']]]]}}" bind:cancelModal="__e" bind:comfirmModal="__e" bind:__l="__l"></alert-box><search-form class="vue-ref" vue-id="3" dataSource="{{dataSource}}" data-ref="searchForm" data-event-opts="{{[['^getSelectDataInfo',[['getSelectDataInfo']]]]}}" bind:getSelectDataInfo="__e" bind:__l="__l"></search-form></view>