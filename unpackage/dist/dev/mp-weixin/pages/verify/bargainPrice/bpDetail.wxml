<view><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">特价详情</view></cu-custom><view><view class="grid-warp"><view class="grid-title">特价详情</view><view class="grid-body"><view class="grid-flex padding-10"><view><label class="_span">{{"客户名称:"+detailItems.co_CustName}}</label></view></view><view class="grid-flex padding-10"><view><label class="_span">{{"单号:"+detailItems.co_No}}</label></view><view><label class="_span">{{"日期:"+$root.m0}}</label></view></view><view class="grid-flex padding-10"><view><label class="_span">{{"纸质:"+detailItems.co_ArtId}}</label></view><view><label class="_span">{{"愣别:"+detailItems.co_ArtLB}}</label></view></view><view class="grid-flex padding-10"><view><label class="_span">优惠:</label></view><view><label class="_span">优惠利率:</label></view></view><view class="grid-flex padding-10"><view><label class="_span">{{"纸长:"+detailItems.co_CSize_l}}</label></view><view><label class="_span">{{"纸宽:"+detailItems.co_CSize_w}}</label></view></view><view class="grid-flex padding-10"><view><label class="_span">{{"数量:"+detailItems.co_Qty}}</label></view></view><view class="grid-flex padding-10"><view><label class="_span"></label></view><view><label class="_span"></label><label class="text-price _span">{{detailItems.co_SPecPrice}}</label></view></view></view></view></view><view class="{{['cu-modal',idShowModal?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content">{{dialogTitle}}</view><view data-event-opts="{{[['tap',[['cancelDialog',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl"><textarea maxlength="60" placeholder="请填写备注内容..." data-event-opts="{{[['input',[['textareaAInput',['$event']]]]]}}" bindinput="__e"></textarea></view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['cancelDialog',['$event']]]]]}}" class="cu-btn line-green text-green" bindtap="__e">取消</button><button data-event-opts="{{[['tap',[['okDialog',['$event']]]]]}}" class="cu-btn bg-green margin-left" bindtap="__e">确定</button></view></view></view></view><view class="button-group cu-bar bg-white tabbar border shop"><view class="btn-group"><button class="cu-btn bg-blue round shadow-blur lg" disabled="{{btn_disabled}}" data-event-opts="{{[['tap',[['openDialog',['resolve']]]]]}}" bindtap="__e">同意</button><button class="cu-btn bg-red round shadow-blur lg" disabled="{{btn_disabled}}" data-event-opts="{{[['tap',[['openDialog',['reject']]]]]}}" bindtap="__e">驳回</button></view></view></view>