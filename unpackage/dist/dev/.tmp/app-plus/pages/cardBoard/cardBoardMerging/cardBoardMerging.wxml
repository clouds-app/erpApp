<view><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">卡板合并</view></cu-custom><form><view data-ref="elForm" class="elForm vue-ref"><view class="cu-form-group border-top"><view class="title">工ㅤ单ㅤ号:</view><input placeholder="请扫描工单号" name="input" data-event-opts="{{[['blur',[['checkWorkNoInfo',['$event']]]],['input',[['__set_model',['$0','workNo','$event',[]],['formItem']]]]]}}" value="{{formItem.workNo}}" bindblur="__e" bindinput="__e"/><button class="cu-btn bg-green shadow" loading="{{false}}" disabled="{{false}}" type data-event-opts="{{[['tap',[['turnOnScanCode',['workNo']]]]]}}" bindtap="__e">扫描</button></view><view class="cu-form-group border-top"><view class="title">原ㅤ卡ㅤ板:</view><input disabled="{{false}}" placeholder="请选择原卡板" name="input" data-event-opts="{{[['blur',[['getCardBoardNoInfo',['$event']]]],['input',[['__set_model',['$0','cardBoardNo','$event',[]],['formItem']]]]]}}" value="{{formItem.cardBoardNo}}" bindblur="__e" bindinput="__e"/><button class="cu-btn bg-green shadow" loading="{{false}}" disabled="{{canScanCardBoard}}" type data-event-opts="{{[['tap',[['turnOnScanCode',['cardBoardNo']]]]]}}" bindtap="__e">扫描</button></view><view class="cu-form-group border-top"><view class="title">原卡板数量:</view><input disabled placeholder="暂无数量" name="input" data-event-opts="{{[['input',[['__set_model',['$0','cardBoardNum','$event',[]],['formItem']]]]]}}" value="{{formItem.cardBoardNum}}" bindinput="__e"/></view><view class="cu-form-group border-top margin-top"><view class="title">新 卡 板 号:</view><input placeholder="请扫新卡板号" name="input" data-event-opts="{{[['blur',[['getNewCardBoardNoInfo',['$event']]]],['input',[['__set_model',['$0','newcardBoardNo','$event',[]],['formItem']]]]]}}" value="{{formItem.newcardBoardNo}}" bindblur="__e" bindinput="__e"/><button class="cu-btn bg-green shadow" loading="{{false}}" disabled="{{canScanCardBoard}}" type data-event-opts="{{[['tap',[['turnOnScanCode',['newcardBoardNo']]]]]}}" bindtap="__e">扫描</button></view><view class="cu-form-group border-top"><view class="title">新卡板数量:</view><input disabled placeholder="暂无数量" name="input" data-event-opts="{{[['input',[['__set_model',['$0','newcardBoardNum','$event',[]],['formItem']]]]]}}" value="{{formItem.newcardBoardNum}}" bindinput="__e"/></view><view class="cu-form-group margin-top"><view class="title">整理后数量:</view><input disabled placeholder="暂无数量" name="input" value="{{totalMergingNum}}"/></view></view></form><view style="{{'height:'+(tableHeight-10+'px')+';'}}"></view><view data-ref="elBtn" class="elBtn margin-text-center vue-ref"><view class="flex  p-xs "><view class="flex-twice  radius"></view><view class="flex-sub radius"></view><view class="flex-twice  radius"><button class="cu-btn block bg-green  lg" loading="{{false}}" disabled="{{canSubmitData}}" type data-event-opts="{{[['tap',[['preSubmitData']]]]}}" bindtap="__e">确定合并</button></view></view></view><alert-box class="vue-ref" vue-id="2" msgContent="{{msgContent}}" toShowModal="{{toShowModal}}" data-ref="alertBox" data-event-opts="{{[['^cancelModal',[['cancelModal']]],['^comfirmModal',[['comfirmModal']]]]}}" bind:cancelModal="__e" bind:comfirmModal="__e" bind:__l="__l"></alert-box><search-form class="vue-ref" vue-id="3" dataSource="{{dataSource}}" data-ref="searchForm" data-event-opts="{{[['^getSelectDataInfo',[['getSelectDataInfo']]]]}}" bind:getSelectDataInfo="__e" bind:__l="__l"></search-form></view>