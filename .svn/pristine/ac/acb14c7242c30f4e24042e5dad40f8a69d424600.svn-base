 let apiPath= '/clerp-app-api'  //正式环境 
/**
 * @description 獲取通知信息個數
 * @params {url} 動態地址查詢
 */
export const getNoticeInfo = ({url}) => {
	//参数
	let data = {
		isLoad:'false',
	}

	
	return axios.request({
		url: `${apiPath}${url}`,
		data,
		method: 'POST',
	})
}

