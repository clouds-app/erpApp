/**
 * @description 纸箱特价审批
 * @params {}
 */
export const boxDetailApproval = ({
	Id,
	approvalExplain,
	approveState
}) => {
	//参数
	let data = {
		ID1:Id,
		bi_SPriceAudit:approveState,	//1:审批，0:未审批
		bi_SPriceAuditExplain:approvalExplain	//审批说明
	}

	return axios.request({
		url: `${apiPath}/confirm/priceAudit/priceAudit`,
		data,
		method: 'POST',
	})
}

